// Character descriptors for Arial 16pt
// { [Char width in bits], [Offset into arial_16ptCharBitmaps in bytes] }
const FONT_CHAR_INFO font_16ptChrsDescr[] =
{
	{10, 0}, 		// 0
	{6, 30}, 		// 1
	{10, 45}, 		// 2
	{10, 75}, 		// 3
	{10, 105}, 		// 4
	{10, 135}, 		// 5
	{10, 165}, 		// 6
	{10, 195}, 		// 7
	{10, 225}, 		// 8
	{10, 255}, 		// 9
};

// Font information for Arial 16pt
